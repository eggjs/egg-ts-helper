{
  "name": "egg-ts-helper",
  "version": "1.33.0",
  "description": "egg typescript helper",
  "bin": {
    "ets": "dist/bin.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "register.js"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/egg-ts-helper.git"
  },
  "scripts": {
    "build": "tsc -d",
    "build:w": "tsc -d -w",
    "release": "del dist && npm run build",
    "lint": "eslint . --ext .ts",
    "check": "npm run build && npm run lint",
    "test": "npm run check && npm run test-local",
    "test-local": "egg-bin test --ts",
    "prepublish": "npm run release",
    "cov": "egg-bin cov --ts",
    "ci": "npm run check && npm run cov"
  },
  "keywords": [
    "egg",
    "typescript"
  ],
  "author": "wanghx",
  "license": "MIT",
  "dependencies": {
    "cache-require-paths": "^0.3.0",
    "chalk": "^2.4.2",
    "chokidar": "^3.0.0",
    "commander": "^2.15.1",
    "debug": "^4.3.4",
    "dot-prop": "^4.2.0",
    "enquirer": "^2.3.0",
    "globby": "^11.0.0",
    "json5": "^2.2.0",
    "mkdirp": "^0.5.1",
    "ts-node": "^7.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "yn": "^3.0.0"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "^1.0.0",
    "@types/commander": "^2.12.2",
    "@types/del": "^3.0.0",
    "@types/globby": "^6.1.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^10.0.0",
    "@types/node": "^9.4.5",
    "del": "^3.0.0",
    "del-cli": "^1.1.0",
    "egg": "^3.5.0",
    "egg-bin": "^5.5.0",
    "egg-ci": "^2.2.0",
    "egg-mock": "^5.2.1",
    "egg-sequelize": "^4.3.1",
    "eslint": "^8.28.0",
    "eslint-config-egg": "^12.1.0",
    "extend2": "^1.0.0",
    "runscript": "^1.3.0"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "ci": {
    "version": "14, 16, 18",
    "os": "linux"
  }
}
